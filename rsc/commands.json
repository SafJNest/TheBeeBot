    "setwelcomemessage":{
        "alias" : ["setwelcome", "welcome"],
        "help"  : "Allow to set a 'welcome quote' that would be sent every time a user join the server. Its necessary to set also a channel otherwise will be used the default system channel. Can also be setted only one role (for now) to be given. Ex: psetwelcome #welcome @role | Hello #user!. Using #user the user will be mentioned. - Write disable to cancel the function.",
        "category" : "Settings",
        "arguments" : "[CommandName] (#canale)(@ruoli) | [messaggio]"
    },

    "setroom":{
        "alias" : ["room"],
        "help"  : "Allow to set a nickname for the channel, for an easier use of 'move'.",
        "category" : "Settings",
        "arguments" : "[CommandName] [#canale] || [CommandName] [idCanale] "
    },

    "setvoice":{
        "alias" : ["voice"],
        "help"  : "Allow to set a default voice for command 'tts'. Every bot will have a different setted voice.",
        "category" : "Settings",
        "arguments" : "[CommandName] [nomeVoce]"
    },


    "setprefix":{
        "alias" : ["prefix"],
        "help"  : "Changes the prefix of the guild",
        "category" : "Settings",
        "arguments" : "[CommandName] [new Prefix]"
    },

    "setsummoner":{
        "alias" : ["setsum", "setuser", "ss"],
        "help"  : "Allow to connect a riot account. You can also remove a summoner using 'setsummoner remove:name'.",
        "category" : "Settings",
        "arguments" : "[CommandName] [summonerName]"
    },

    "setlevelupmessage":{
        "alias" : ["setlevelup", "setlvl"],
        "help"  : "Allow to set a 'level up message' to be sent every time a user level up in the server. Its necessary to set also a channel otherwise will be used the default system channel.Ex: psetlevelupmessage #global_chat | WP #user!. Using #user the user will be mentioned. This work only for Beebot. - Write disable to cancel the function.",
        "category" : "Settings",
        "arguments" : "[CommandName] (#channel) | [messagge]"
    },

    "setleavemessage":{
        "alias" : ["setleave", "leave"],
        "help"  : "Allow to set a message to be sent every time a user left a guild. Its necessary to set also a channel otherwise will be used the default system channel.Ex: psetleft #global_chat | WP #user!. Using #user the user will be mentioned. This work only for Beebot. - Write disable to cancel the function.",
        "category" : "Settings",
        "arguments" : "[CommandName] (#channel) | [message]"
    },










