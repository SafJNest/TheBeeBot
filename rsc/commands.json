    "unmute":{
        "alias" : ["listentothesound"],
        "help"  : "Unmute a member.",
        "category" : "Member Management",
        "arguments" : "[unmute] [@user]"
    },

    "move":{
        "alias" : ["mv", "throw"],
        "help"  : "Move a member from a voice channel to another one.\nIts possible to move multiple users at once.",
        "category" : "Member Management",
        "arguments" : "[CommandName] (here = all the member in the voice channel will be moved) (channel id) | (me = you) (channel id) | (member) (channel id) | (member) (here = the user will be move where you are) | (member) (afk)." 
    },

    "modifynickname":{
        "alias" : ["modnick", "nick"],
        "help"  : "Change the nickname for a member.",
        "category" : "Member Management",
        "arguments" : "[CommandName] [member] [new nick]"
    },













   

    "calc":{
        "alias" : ["c", "calculator","calculate"],
        "help"  : "Does several mathematical operation.\nSum, subtraction, multiplication, division and exponentiation.\nThe function are square root, sine, cosine, exponential and their inverse.",
        "category" : "Math",
        "arguments" : "[calc] [(n+n) (n-n) (n*n) (n/n) (n^n) (sqrt n) (sin n) (cos n ) (ln n) (tan n) (asin n) (acos n) (exp n)]"
    },

    

    "dice":{
        "alias" : ["aleaiactaest", "roll"],
