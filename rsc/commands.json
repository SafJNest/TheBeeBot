{
    "connect":{
        "alias" : ["join", "comeherebroda"],
        "help"  : "Connect the bot to the voice channel.",
        "category" : "Audio",
        "arguments" : "[CommandName]"
    },

    "customizesound":{
        "alias" : ["custom", "cs", "personalize"],
        "help"  : "WIP",
        "category" : "Audio",
        "arguments" : "[CommandName]"
    },

    "deletesound":{
        "alias" : ["delete", "ds", "forget"],
        "help"  : "Remove a sound from the database.",
        "category" : "Audio",
        "arguments" : "[CommandName]"
    },

    "disconnect":{
        "alias" : ["bye", "fuckoff"],
        "help"  : "Disconnect the bot or a user from the voice channel.",
        "category" : "Audio",
        "arguments" : "[CommandName]"
    },

    "list":{
        "alias" : ["papyrus", "paper", "listoide"],
        "help"  : "Display a list with all the sound in a server.\nUsing these arguments you can get:\nme = all the sounds you have uploaded.\nuser @user/IDuser = all the sounds a user has upload.\nglobal = all the sounds in the DB.",
        "category" : "Audio",
        "arguments" : "[CommandName] | [me] | [user @user/IDuser] | [global]"
    },

    "playyoutube":{
        "alias" : ["y", "py", "nuovavita", "yt"],
        "help"  : "Play a video from youtube.",
        "category" : "Audio",
        "arguments" : "[CommandName] [link youtube]"
    },

    "playsound":{
        "alias" : ["p", "ps"],
        "help"  : "Play a custom sound from the database.",
        "category" : "Audio",
        "arguments" : "[CommandName] [soundName]"
    },

    "tts":{
        "alias" : ["t", "talk"],
        "help"  : "Read what you write.",
        "category" : "Audio",
        "arguments" : "[CommandName] [text]"
    },

    "stop":{
        "alias" : ["stp", "donteventhinkaboutit"],
        "help"  : "Stop a song.",
        "category" : "Audio",
        "arguments" : "[CommandName]"
    },

    "upload":{
        "alias" : ["up", "add"],
        "help"  :"Upload a custom sound to the database.",
        "category" : "Audio",
        "arguments" : "[CommandName] [soundName]"
    },

    "downloadsound":{
        "alias" : ["download"],
        "help"  : "Download a sound.",
        "category" : "Audio",
        "arguments" : "[CommandName] [soundName]"
    },














    "channelinfo":{
        "alias" : ["infochannel", "ci","ic","channel"],
        "help"  : "Show the information about a channel.",
        "category" : "Server Management",
        "arguments" : "[channelinfo] [#channel/channel ID]"
    },

    "clear":{
        "alias" : [],
        "help"  : "Delete the message in a channel (max 99).",
        "category" : "Server Management",
        "arguments" : "[clear] [numberMessages]"
    },

    "emojiinfo":{
        "alias" : ["em", "eminf"],
        "help"  : "Show the information about an emoji.",
        "category" : "Server Management",
        "arguments" : "[emoji] [emojiName, emoji]"
    },
    
    "serverinfo":{
        "alias" : ["servinfo", "serinf"],
        "help"  : "Show the information about the server.",
        "category" : "Server Management",
        "arguments" : "[CommandName]"
    },

    "userinfo":{
        "alias" : ["usinf", "uinfo"],
        "help"  : "Show the information aboit a user.",
        "category" : "Server Management",
        "arguments" : "[userinfo] [@user]"
    },

    "setwelcome":{
        "alias" : [],
        "help"  : "Allow to set a 'welcome quote' to be sent every time a user join the server. Its necessary to set also a channel otherwise will be used the default system channel. Can also be setted roles to given. Ex: psetwelcome #welcome @role | Hello #user!. Using #user the user will be mentioned.",
        "category" : "Server Management",
        "arguments" : "[CommandName] (#canale)(@ruoli) | [messaggio]"
    },

    "setroom":{
        "alias" : ["room"],
        "help"  : "Allow to set a nickname for the channel, for an easier use of 'move'.",
        "category" : "Server Management",
        "arguments" : "[CommandName] [#canale] || [CommandName] [idCanale] "
    },

    "setvoice":{
        "alias" : ["voice"],
        "help"  : "Allow to set a default voice for command 'tts'.",
        "category" : "Server Management",
        "arguments" : "[CommandName] [nomeVoce]"
    },

    "listroom":{
        "alias" : ["rooms"],
        "help"  : "List with all the nickname in a server.",
        "category" : "Server Management",
        "arguments" : "[CommandName] [#canale] || [CommandName] [idCanale] "
    },


















    "ban":{
        "alias" : ["sgozz", "destroy", "annihilate", "radiateDeath", "drinkRadithorForAYear", "ohthemisery"],
        "help"  : "Ban a user.",
        "category" : "Member Management",
        "arguments" : "[ban] [@user] (reason)"
    },

    "image":{
        "alias" : ["img"],
        "help"  : "Show the imagine of a user.",
        "category" : "Member Management",
        "arguments" : "[image] [@user]"
    },

    "kick":{
        "alias" : ["lightdeath", "espulsione", "takethetrashout"],
        "help"  : "Kick a member.",
        "category" : "Member Management",
        "arguments" : "[kick] [@user]"
    },

    "mute":{
        "alias" : ["dontlistentothesound"],
        "help"  : "Mute a member.",
        "category" : "Member Management",
        "arguments" : "[mute] [@user]"
    },

    "permissions":{
        "alias" : ["perms", "permission"],
        "help"  : "Show all the permissions a user has.",
